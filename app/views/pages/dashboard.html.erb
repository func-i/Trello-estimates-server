<h1>
  My Projects
</h1>

<table class="table">
  <thead>
    <th>
      Name
    </th>
    <th>
      Developers Estimation
    </th>
    <th>
      Managers Estimation
    </th>
    <th>
      Total Tracked Time
    </th>
    <th>
      Hours Left
    </th>
    <th>
      Performance
    </th>
  </thead>
  <tbody>
    <% @projects.each do |board| %>
      <%= render "board", :board => board %>
    <% end %>

  </tbody>
</table>

